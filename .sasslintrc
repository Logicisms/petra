{
    "rules": {
        "class-name-format": [
            1,
            {
            "allow-leading-underscore": false
            }
        ],
        "declarations-before-nesting": 1,
        "empty-args": 0,
        "indentation": [
            2,
            {
                "size": 4
            }
        ],
        "leading-zero": [
            1,
            {
            "include": true
            }
        ],
        "no-color-keywords": 0,
        "no-color-literals": 2,
        "no-css-comments": 0,
        "no-debug": 1,
        "no-duplicate-properties": [
            2,
            {
                "exclude": []
            }
        ],
        "no-empty-rulesets": 1,
        "no-extends": 2,
        "no-ids": 2,
        "no-important": 0,
        "no-invalid-hex": 2,
        "no-misspelled-properties": [
            2,
            {
                "extra-properties": []
            }
        ],
        "no-qualifying-elements": 0,
        "no-trailing-whitespace": 1,
        "no-trailing-zero": 1,
        "no-universal-selectors": 1,
        "no-vendor-prefixes": [
            2,
            {
                "additional-identifiers": [],
                "excluded-identifiers": []
            }
        ],
        "no-warn": 0,
        "property-sort-order": [
            0,
            {
                "order": [],
                "ignore-custom-properties": true
            }
        ],
        "pseudo-element": 2,
        "single-line-per-selector": 1,
        "space-between-parens": 0,
        "url-quotes": 2,
        "zero-unit": 2
    }
}
